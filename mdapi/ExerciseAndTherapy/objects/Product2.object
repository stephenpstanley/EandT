<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Maximum_Class_Size__c</fullName>
        <description>The maximum number of people who can attend a class</description>
        <externalId>false</externalId>
