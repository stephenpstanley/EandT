        <label>Maximum Class Size</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <validationRules>
        <fullName>Greater_than_zero</fullName>
        <active>true</active>
        <description>If not null, then must be &gt; 0</description>
        <errorConditionFormula>Maximum_Class_Size__c &lt; 1</errorConditionFormula>
        <errorDisplayField>Maximum_Class_Size__c</errorDisplayField>
        <errorMessage>If a number is entered for Maximum Class Size, then it must be greater than zero</errorMessage>
    </validationRules>
</CustomObject>
